{
  "env": {
    "FIREBASE_PROJECT_ID": "blog-88f73",
    "FIREBASE_PRIVATE_KEY": "@blog-firebase-private-key",
    "FIREBASE_CLIENT_EMAIL": "@blog-firebase-client-email"
  },
  "routes": [
    {
      "src": "/atom\\/?",
      "headers": {
        "content-type": "text/xml"
      }
    },
    {
      "src": "/essays.*",
      "headers": {
        "location": "/"
      },
      "status": 302
    },
    {
      "src": "/slackin\\/?.*",
      "headers": {
        "location": "https://github.com/rauchg/slackin"
      },
      "status": 302
    }
  ]
}